		"build": "tsc"
	},
	"dependencies": {
		"esbuild": "^0.19.10",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@types/node": "^20.10.5",
		"npm-run-all": "^4.1.5"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=16.0.0"
	}
}
